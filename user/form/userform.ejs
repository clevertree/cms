<%
    if(typeof form === 'undefined')
        form = 'profile'
    switch(form) {
        case 'profile':
        case 'flags':
        case 'password':
            if(typeof id === 'undefined')
                throw new Error("Form " + form + " requires id attribute");
%>
<script src="/user/form/userform-update-<%=form%>.client.js"></script>
<userform-update-<%=form%> id="<%=id%>"></userform-update-<%=form%>>
<%
            break;

    case 'login':
    case 'forgotpassword':
    case 'logout':
    case 'register':
%>
<script src="/user/form/userform-<%=form%>.client.js"></script>
<userform-<%=form%>></userform-<%=form%>>
<%
            break;
    default:
            throw new Error("Invalid userform: " + form);
    }
%>
